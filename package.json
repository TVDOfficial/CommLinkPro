{
  "name": "commlink-pro",
  "version": "1.1.0",
  "description": "CommLink Pro - Enterprise Communication Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "node setup.js",
    "demo": "node demo-data.js",
    "migrate": "node migrate-radio-id.js",
    "reset": "rm -f radio_registry.db && echo 'Database reset. Run npm start to recreate.'"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "moment": "^2.29.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["radio", "registry", "inventory", "management"],
  "author": "Radio Registry System",
  "license": "MIT"
}